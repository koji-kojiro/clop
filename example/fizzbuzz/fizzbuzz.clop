(include stdio)

(defun void:fizzbuzz (int:n)
  (cond
    ((eq (mod n 15) 0) (printf "fizzbuzz\n"))
    ((eq (mod n 5) 0) (printf "buzz\n"))
    ((eq (mod n 3) 0) (printf "buzz\n"))
    (1 (printf "%d\n" n))))

(defun int:main (void)
  (declare int:n)
  (for ((setf n 0) (< n 30) (incf n))
    (fizzbuzz (1+ n)))
  (return 0))
