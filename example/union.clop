(include stdio)

;; union person
(defunion person
  (char*:name int:age))

;; main function 
(defun int:main (int:argc char*:argv[])
  (let ((person:kojiro)
        (kojiro.name "Kojiro Tani"))
    (printf "My name is %s.\n" kojiro.name)
    (set kojiro.age 25)
    (printf "I am %d years old.\n" kojiro.age))
  (return 0))

